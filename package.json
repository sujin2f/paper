{
    "name": "ether-hypothesis",
    "version": "3.0.0",
    "description": "The space and time of the matter",
    "license": "MIT",
    "scripts": {
        "dev": "npm-run-all -s dev:clean dev:watch",
        "dev:clean": "rimraf .build/development/*",
        "dev:watch": "concurrently --kill-others-on-fail \"yarn dev:node\" \"yarn dev:react\"",
        "dev:node": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn ./src/server/index.ts",
        "dev:react": "NODE_ENV=development webpack --watch",
        "stage": "npm-run-all -s stage:clean stage:tsc stage:webpack",
        "stage:clean": "rimraf .build/stage/*",
        "stage:webpack": "NODE_ENV=stage webpack",
        "stage:tsc": "NODE_ENV=stage tsc --project tsconfig.stage.json",
        "stage:server": "NODE_ENV=stage node .build/stage/server/index.js",
        "prod": "npm-run-all -s prod:clean prod:tsc prod:webpack",
        "prod:clean": "rimraf .build/production/*",
        "prod:webpack": "NODE_ENV=production webpack",
        "prod:tsc": "NODE_ENV=production tsc --project tsconfig.production.json",
        "prod:server": "NODE_ENV=production node .build/production/server/index.js",
        "prod:analyze": "source-map-explorer '.build/production/frontend/static/js/*.js'",
        "test": "NODE_ENV=development jest",
        "lint": "eslint './src/**/*.ts' './src/**/*.tsx'",
        "pretty": "yarn prettier -w ./src"
    },
    "dependencies": {},
    "devDependencies": {
        "@apollo/client": "^3.7.1",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/connect-timeout": "^0.0.36",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.2.3",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^18.7.3",
        "@types/react": "^18.0.25",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "^18.0.9",
        "@types/react-router-dom": "^5.3.3",
        "@types/supertest": "^2.0.12",
        "axios": "^1.2.1",
        "concurrently": "^7.6.0",
        "connect-timeout": "^1.9.0",
        "csv-parse": "^5.3.2",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.8",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "express": "^4.18.1",
        "foundation-sites": "6.6.2",
        "graphql": "^16.6.0",
        "graphql-http": "^1.17.1",
        "http": "^0.0.1-security",
        "https": "^1.0.0",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "module-alias": "^2.2.2",
        "mongoose": "^6.7.3",
        "node-cache": "^5.1.2",
        "node-sass": "^9.0.0",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "prettier": "^2.8.1",
        "react": "^18.2.0",
        "react-color": "^2.19.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.3",
        "react-scripts": "^5.0.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^14.1.1",
        "source-map-explorer": "^2.5.3",
        "supertest": "^6.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.1.0",
        "typescript": "4.7.4",
        "webpack": "^5.90.3",
        "webpack-cli": "^4.10.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
